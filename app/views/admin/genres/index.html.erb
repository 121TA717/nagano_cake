<h2>ジャンル一覧・追加</h2>

  <%= form_with model: @genre, url: admin_genres_path do |f| %>
  <div class="field">
    <%= f.label 'ジャンル名' %>
    <%= f.select :genres, autofocus: true, autocomplete: "ジャンル名" %>
    <%= link_to '新規登録', edit_admin_genre_path, class: "btn btn-success" %>
  </div>
  <% end %>



